{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-read-text-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "http:default",
    "http:allow-fetch",
    "opener:default",
    "opener:allow-open-url",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:11434/*"
        },
        {
          "url": "https://*"
        }
      ]
    }
  ]
}
